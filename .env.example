# Database Configuration
# PostgreSQL connection URL
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/field_solutions

# Database connection parameters (used in docker-compose)
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=field_solutions
DB_PORT=5432

# FSM API Configuration
# Required: API key for Field Solutions Manager API
FSM_API_KEY=your_fsm_api_key_here

# Optional: FSM API URL (defaults to https://api.fieldsolutionsmanager.com)
FSM_API_URL=https://api.fieldsolutionsmanager.com

# Optional LLM Configuration
# LLM API key for AI/ML features (optional)
LLM_API_KEY=

# Application Configuration
# Backend port for docker-compose
BACKEND_PORT=8000

# Debug mode
DEBUG=false
